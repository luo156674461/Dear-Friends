{
    "presets": ["es2015", "stage-2", ["env",
        {
            "modules": false,
            "useBuiltIns": "entry",
            "targets":
            {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }]],
    "plugins": ["transform-runtime", "transform-exponentiation-operator", "transform-vue-jsx", ["import",
    {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
    }]],
    "comments": false,
    "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}